---
import MainPage from "../../layouts/MainPage.astro";
import { getCustomStaticPathsGetter } from "../../utils";

export async function getStaticPaths({ paginate }) {
	const __getStaticPaths = await getCustomStaticPathsGetter(
		await Astro.glob('../blog/*/*/*/*.mdx'),
		'tail',
		'/old',
		false
	);
	return __getStaticPaths({ paginate })
}

const { page } = Astro.props;
---

<MainPage page={page}></MainPage>
